{
  "name": "rewiseai-beta",
  "private": true,
  "version": "1.0.0-beta",
  "description": "AI-powered flashcard generation platform for transforming PDF study materials into interactive learning experiences",
  "keywords": [
    "ai",
    "flashcards",
    "education",
    "learning",
    "pdf",
    "react",
    "typescript",
    "firebase",
    "gemini"
  ],
  "author": "Ruth Wiki",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruthwikiit2020/beta-testing.git"
  },
  "homepage": "https://rewiseai.com",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "vercel --prod",
    "deploy:firebase": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.2.1",
    "framer-motion": "^11.3.11",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.544.0",
    "razorpay": "^2.9.6",
    "react": "^19.1.1",
    "react-activity-calendar": "^2.7.15",
    "react-dom": "^19.1.1",
    "react-github-calendar": "^4.5.11",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "netlify-cli": "^23.6.0",
    "postcss": "^8.5.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  }
}
